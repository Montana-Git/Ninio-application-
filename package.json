  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build.js",
    "build-no-validate": "tsc && node --max-old-space-size=4096 ./node_modules/vite/bin/vite.js build",
    "build-low-memory": "node scripts/build-low-memory.js",
    "build-simple": "node scripts/simple-build.js",
    "build-minimal": "node scripts/minimal-build.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:component": "cypress open --component",
    "analyze": "vite build --mode analyze",
    "analyze:report": "node scripts/analyze-bundle.js",
    "type-check": "tsc --noEmit",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:production": "node scripts/deploy.js production"
